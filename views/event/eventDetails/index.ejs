                    </li>
                </ul>

            </div>
        </div>
    </nav>
      <!-- banner start-->
      <section class="hero-area banner-6 banner-6-alt" style="height: 100vh;">
         <div class="banner-item">
            <div class="container-fluid" >
               <div class="row" style="height: 100vh; background-image: url(/images/bg-eve.jpg); background-size: cover; ">
                  <div class="col-lg-6 ms-3">
                     <div class="banner-content-wrap">
                        <h2 class="banner-title wow fadeInUp" data-wow-duration="1.5s" data-wow-delay="700ms">
                            <%= event.titre %>
                        </h2>
                     </div>
                     <!-- Banner content wrap end -->
                  </div><!-- col end-->
               </div><!-- row end-->
            </div>
            <!-- Container end -->
         </div>
      </section>
      <!-- banner end-->
      <section class="container mt-5">
        <!-- Event Title and Description -->
        <div class="row featurette">
            <div class="col-md-7">
                <h2 class="featurette-heading fw-normal lh-1"><%= event.titre %></h2>
                <p class="lead" style="margin-top: 75px;"><%= event.description %></p>
            </div>
            <div class="col-md-5">
                <svg class="bd-placeholder-img bd-placeholder-img-lg featurette-image img-fluid mx-auto" width="500" height="500" xmlns="http://www.w3.org/2000/svg" role="img" aria-label="Placeholder: 500x500" preserveAspectRatio="xMidYMid slice" focusable="false">
                    <title>Placeholder</title>
                    <rect width="100%" height="100%" fill="var(--bs-secondary-bg)" />
                    <text x="50%" y="50%" fill="var(--bs-secondary-color)" dy=".3em">500x500</text>
                </svg>
            </div>
        </div>
    
        <!-- Event Details -->
        <p><strong>Date de début :</strong> <%= event.date_debut %></p>
        <p><strong>Date de fin :</strong> <%= event.date_fin %></p>
        <p><strong>Lieu :</strong> <%= event.lieu %></p>
    
        <!-- Sponsors Section -->
        <h2 class="mt-5">Sponsors</h2>
        <div class="row">
            <% sponsors.forEach(sponsor => { %>
            <div class="col-lg-4 text-center mb-4">
                <img src="/images/bg-2.jpeg" alt="<%= sponsor.nom %>" style="width: 140px; height: 140px; border-radius: 50%;">
                <h2 class="fw-normal mt-3"><%= sponsor.nom %></h2>
                <p><%= sponsor.description %></p>
                <p><a class="btn btn-secondary" href="#">View details &raquo;</a></p>
            </div>
            <% }) %>
        </div>
    
        <!-- Speakers Section -->
        <h2 class="mt-5">Speakers</h2>
        <div class="row">
            <% speakers.forEach(speaker => { %>
            <div class="col-lg-4 text-center mb-4">
                <% if (speaker.image_url) { %>
                <img src="<%= speaker.image_url %>" alt="<%= speaker.nom %> <%= speaker.prenom %>" style="width: 140px; height: 140px; border-radius: 50%;">
                <% } else { %>
                <img src="/images/default-speaker.png" alt="<%= speaker.nom %> <%= speaker.prenom %>" style="width: 140px; height: 140px; border-radius: 50%;">
                <% } %>
                <h2 class="fw-normal mt-3"><%= speaker.nom %> <%= speaker.prenom %></h2>
                <p><%= speaker.description %></p>
                <p><a class="btn btn-secondary" href="#">View details &raquo;</a></p>
            </div>
            <% }) %>
        </div>
    
        <!-- Programme Section -->
        <h2 class="mt-5">Programme</h2>
        <div class="row">
            <% programmes.forEach(programme => { %>
            <div class="col-md-6">
                <div class="card mb-4">
                    <div class="card-body">
                        <h5 class="card-title"><strong>Date:</strong> <%= programme.titre %></h5>
                        <p class="card-text"><%= programme.description %></p>
                    </div>
                </div>
            </div>
            <% }) %>
        </div>
    
        <!-- Apply Button -->
        <div class="d-flex justify-content-center mt-4">
            <button class="btn btn-primary">Demander Condidature</button>
        </div>
    </section>  
    
            <!-- footer start-->
